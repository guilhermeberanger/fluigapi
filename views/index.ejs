<!doctype html>
<html lang="pt-br">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <link href="https://fonts.googleapis.com/css?family=Roboto:400,700,900&display=swap" rel="stylesheet">

  <link rel="stylesheet" href="fonts/icomoon/style.css">

  <!-- Bootstrap CSS -->
  <link rel="stylesheet" href="css/bootstrap.min.css">

  <!-- Style -->
  <link rel="stylesheet" href="css/style.css">

  <!-- Adicionando JQuery -->
  <script src="https://code.jquery.com/jquery-3.6.0.min.js"
    integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>

  <!--===============================================================================================-->

  <title>Grupo TB</title>
</head>

<body>


  <div class="content">
    <div class="container">
      <div class="row align-items-stretch no-gutters contact-wrap">
        <div class="col-md-8">
          <div class="form h-100">
            <h3>PORTAL DE CADASTROS</h3>
            <div class="panel panel-primary"></div>
            <div class="row">
              <div class="form-group col-md-6">
                <input class="form-control" placeholder="Selecione abaixo o tipo de Cadastro">
              </div>
            </div>
            <div class="row">
              <div class="inputs">
                <br>
                <div class="col-md-12">
                  <button type="submit" class="btn btn-primary rounded-0 py-2 px-4"
                    onclick='cadastroProduto();'>PRODUTOS</button>
                  <span class="submitting"></span>
                </div>
              </div>
              <div class="inputs">
                <br>
                <div class="col-md-12">
                  <button type="submit" class="btn btn-primary rounded-0 py-2 px-4"
                    onclick='cadastroProduto();'>FORNECEDORES</button>
                  <span class="submitting"></span>
                </div>
              </div>
            </div>
          </div>

        </div>
        <div class="col-md-4">
          <div class="contact-info h-100">
            <h3>Informações Gerais</h3>
            <p class="mb-5"></p>Prazo para Efetivação do cadastro: 24hrs</p>
            <ul class="list-unstyled">
              <li class="d-flex">
                <span class="wrap-icon icon-users mr-3"></span>
                <span class="text">Equipe Planejamento e Controle</span>
              </li>
              <li class="d-flex">
                <span class="wrap-icon icon-phone mr-3"></span>
                <span class="text">Ramal: 551</span>
              </li>
              <li class="d-flex">
                <span class="wrap-icon icon-envelope mr-3"></span>
                <span class="text">cadastros.pco@grupotb.com.br</span>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Guia de Estilos -->
  <script src="js/jquery-3.3.1.min.js"></script>
  <script src="js/popper.min.js"></script>
  <script src="js/bootstrap.min.js"></script>
  <script src="js/jquery.validate.min.js"></script>
  <script src="js/main.js"></script>
  <script>
    function cadastroProduto() {
      location.href = "https://produtocad.herokuapp.com/produtos"
    }
    function cadastroProduto() {
      location.href = "https://produtocad.herokuapp.com/fornecedor"
    }
  </script>

</body>

</html>